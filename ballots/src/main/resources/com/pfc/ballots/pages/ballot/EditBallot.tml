<html xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
	xmlns:p="tapestry:parameter">
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<meta http-equiv="Cache-control" content="no-cache" />
<meta http-equiv="Expires" content="-1" />
<head>
<title>${message:page-title}</title>
<link rel="icon" type="image/x-icon" href="${context:favicon2.ico}" />
<link rel="icon" href="${context:favicon2.ico}" type="image/x-icon" />
</head>

<t:border>
	<div id="CreateballotDiv">
		<div class="t-zbreadcrumbs">
			<ul class="t-zbreadcrumbs">
				<li><a href="Index">${message:index}</a></li>
				<li><a href="BallotList">${message:ballot}</a></li>
				<li><a href="#">${message:actual}</a></li>
			</ul>
		</div>
		<h1>${message:create-ballot}</h1>
		<hr />

		<t:zone t:id="generalZone" id="generalZone" t:update="show">
			<t:if test="showGeneral">

			</t:if>
		</t:zone>

		<t:zone t:id="typeZone" id="typeZone" t:update="show">

			<t:if test="showtype">
				<t:form t:id="typeForm" zone="^">
					<span class="error"> <t:if test="showErrorType">
							${message:fields-required}<br />
						</t:if> <t:if test="showBadDate">
							${message:bad-date}<br />
						</t:if> <t:if test="showBadName" zone="^">
							<div class="error">
								${message:name-in-use}<br />
							</div>
						</t:if> <t:if test="badValue">
							<span class="error">${message:badValue}</span>
							<br />
						</t:if>
					</span>
					<table>
						<tr>
							<td><t:label for="ballotName">${message:name}*</t:label></td>
							<td><t:textfield t:id="ballotName" value="ballotName" /></td>
						</tr>
						<tr>
							<td><t:label for="description">${message:description}*</t:label>
							</td>
							<td><t:textarea t:id="description"></t:textarea></td>
						</tr>
						<tr>
							<td><t:label for="startDate" /></td>
							<td><t:datefield t:id="startDate"
									format="prop:dateInFormatStr" />(${dateInFormatStr})</td>
						</tr>
						<tr>
							<td><t:label for="startHour" /></td>
							<td><t:textfield t:id="startHour" default="00:00:00" />(hh:mm:ss)</td>
						</tr>
						<tr>
							<td><t:label for="endDate" />*</td>
							<td><t:datefield t:id="endDate"
									format="prop:dateInFormatStr" />(${dateInFormatStr}</td>
						</tr>
						<tr>
							<td><t:label for="endHour" /></td>
							<td><t:textfield t:id="endHour" />(hh:mm:ss)</td>
						</tr>
						<tr>
							<td></td>
							<td><input t:type="submit" class="boton"
								t:value="${message:next-but}" /></td>
						</tr>
					</table>
				</t:form>
			</t:if>

			<!-- ///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
			///////////////////////////////////////////////////////MAYORIA RELATIVA ///////////////////////////////////////////////////////////////////////
		   ///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////	-->


