					<input t:type="submit" class="boton" t:value="${message:create}" />
				</t:form>
				<br />
				<t:eventlink t:event="back" t:context="prop:['fromMayRel','toType']"
					t:zone="^">
					<button class="boton">${message:back-but}</button>
				</t:eventlink>
