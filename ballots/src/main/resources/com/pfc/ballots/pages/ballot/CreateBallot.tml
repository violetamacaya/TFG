					<input t:type="submit" class="boton" t:value="${message:create}" />
				</t:form>
				<br />
				<t:eventlink t:event="back" t:context="prop:['fromRange','toType']"
					t:zone="^">
					<button>${message:back-but}</button>
				</t:eventlink>
			</t:if>
		</t:zone>
		<!-- ///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
			///////////////////////////////////////////////////////// APPROVAL VOTING ///////////////////////////////////////////////////////////////////////////////
		   ///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////	-->



		<t:zone t:id="approvalZone" id="approvalZone" t:update="show">
			<t:if test="showapproval">
				<h4>${message:approval-voting}</h4>
				<t:form t:id="approvalForm" t:zone="^">
					<t:if test="showErrorapproval">
						<span class="error">${message:fill-fields}</span>
						<br />
					</t:if>
					<t:if test="showRepeatedapproval">
						<span class="error">${message:repeated-fields}</span>
						<br />
					</t:if>
					<table>
						<tr>
							<td><t:label for="approvalSel" /></td>
