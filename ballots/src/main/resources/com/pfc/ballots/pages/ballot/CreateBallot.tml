		<!-- ///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
			///////////////////////////////////////////////////////// RANGE_VOTING ///////////////////////////////////////////////////////////////////////////////
		   ///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////	-->


		<t:zone t:id="RangeZone" id="rangeZone" t:update="show">
			<t:if test="showRange">
				<h4>${message:range-title}</h4>
				<t:form t:id="rangeForm" zone="^">
					<span class="error"> <t:if test="showRangeFill">
								${message:fill-fields}<br />
						</t:if> <t:if test="showRangeValues">
								${message:range-values}<br />
						</t:if> <t:if test="showRangeRepeated">
								${message:not-repeated}<br />
						</t:if> <t:if test="showRangeBadChar">
								${message:bad-char}<br />
						</t:if> <t:if test="showRangeBadNum">
								${message:bad-char}<br />
						</t:if>
					</span>
					<table>
						<tr>
							<td><t:label for="rangeSel" /></td>
