<html xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
	xmlns:p="tapestry:parameter">
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<meta http-equiv="Cache-control" content="no-cache" />
<meta http-equiv="Expires" content="-1" />
<head>
<title>${message:page-title}</title>
<link rel="icon" type="image/x-icon" href="${context:favicon2.ico}" />
<link rel="icon" href="${context:favicon2.ico}" type="image/x-icon" />
</head>

<t:border>
	<div id="CreateballotDiv">

		<h1>${message:create-ballot}</h1>
		<hr />

		<t:zone t:id="generalZone" id="generalZone" t:update="show">
			<t:if test="showGeneral">

			</t:if>
		</t:zone>


		<t:zone t:id="typeZone" id="typeZone" t:update="show">
			<t:if test="showtype">
				<t:form t:id="typeForm" zone="^">
					<span class="error"> <t:if test="showErrorType">
							${message:fields-required}<br />
						</t:if> <t:if test="showBadDate">
							${message:bad-date}<br />
						</t:if> <t:if test="showBadName" zone="^">
							<div class="error">
								${message:name-in-use}<br />
							</div>
						</t:if> <t:if test="badValue">
							<span class="error">${message:badValue}</span>
							<br />
						</t:if>
					</span>
					<table>
						<tr>
							<td><t:label for="methodSelect">${message:method}*</t:label></td>
							<td><t:select t:type="select" t:id="methodSelect"
									value="method" t:blankLabel="${message:choose}" t:zone="^" /></td>
						</tr>
						<tr>
							<td><t:label for="ballotKindSelect">${message:Kind}*</t:label></td>
							<td><t:select t:type="select" t:id="ballotKindSelect"
									value="ballotKind" t:blankLabel="${message:choose}" t:zone="^" /></td>
						</tr>
						<tr>
							<td><t:label for="ballotName">${message:name}*</t:label></td>
							<td><t:textfield t:id="ballotName" /></td>
						</tr>
						<tr>
							<td><t:label for="description">${message:description}*</t:label>
							</td>
							<td><t:textarea t:id="description"></t:textarea></td>
						</tr>
						<t:if test="showNormalCensus">
							<t:if test="showExist">
								<t:if test="!showPublic">
									<tr>
										<td><t:label for="censusNormalSelect">${message:census}*</t:label></td>
										<td><t:select t:type="select" t:id="censusNormalSelect"
												t:model="censusNormalModel" t:value="censusNormal"
												t:encoder="censusNormalEncoder" t:blankLabel="Choose..."
												t:zone="^" /></td>
									</tr>
									<p:else>
										<tr>
											<td><t:label for="censusModel">${message:census}</t:label></td>
											<td><t:select t:type="select" t:id="censusPublic"
													t:value="census" t:model="censusModel"
													t:blankOption="ALWAYS" t:blankLabel="${message:choose}" /></td>
										</tr>
									</p:else>
								</t:if>
								<tr>
									<td><t:label for="startDate" /></td>
									<td><t:datefield t:id="startDate"
											format="prop:dateInFormatStr" />(${dateInFormatStr})</td>
								</tr>
								<tr>
									<td><t:label for="startHour" /></td>
									<td><t:textfield t:id="startHour" default="00:00:00" />(hh:mm:ss)</td>
								</tr>
								<tr>
									<td><t:label for="endDate" />*</td>
									<td><t:datefield t:id="endDate"
											format="prop:dateInFormatStr" />(${dateInFormatStr}</td>
								</tr>
								<tr>
									<td><t:label for="endHour" /></td>
									<td><t:textfield t:id="endHour" />(hh:mm:ss)</td>
								</tr>
								<tr>
									<td></td>
									<td><input t:type="submit" class="boton"
										t:value="${message:next-but}" /></td>
								</tr>
								<p:else>
									<tr>
										<td></td>
										<td>${message:no-census} <t:pagelink
												page="census/createCensus">
												<strong>${message:here-link}</strong>
											</t:pagelink></td>
									</tr>
								</p:else>
							</t:if>
							<p:else>
								<t:if test="teaching">
									<tr>
										<td><t:label for="numVotes" /></td>
										<td><t:textfield t:id="numVotes" t:value="census" /></td>

									</tr>
									<p:else>
										<tr>
											<td><t:label for="censusModel">${message:census}*</t:label></td>
											<td><t:select t:type="select" t:id="censusModel"
													t:value="census" t:model="censusModel"
													t:blankOption="ALWAYS" t:blankLabel="${message:choose}" /></td>
										</tr>
									</p:else>
								</t:if>
								<tr>
									<td></td>
									<td><input t:type="submit" class="boton"
										value="${message:next-but}" /></td>
								</tr>
							</p:else>
						</t:if>
					</table>
				</t:form>
			</t:if>

			<!-- ///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
			///////////////////////////////////////////////////////MAYORIA RELATIVA ///////////////////////////////////////////////////////////////////////
		   ///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////	-->


		</t:zone>
		<t:zone t:id="mayRelZone" id="mayRelZone" t:update="show">
			<t:if test="showMayRel">
