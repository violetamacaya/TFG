							</t:if>
							<t:if test="showB1Opt4">
								<tr>
									<td><t:label for="b1Opt4" /></td>
									<td><t:textfield t:id="b1Opt4" /></td>
								</tr>
							</t:if>
							<t:if test="showB1Opt5">
								<tr>
									<td><t:label for="b1Opt5" /></td>
									<td><t:textfield t:id="b1Opt5" /></td>
