								<td><t:label for="approvalOp3" /></td>
								<td><t:textfield t:id="approvalOp3" /></td>
							</tr>
						</t:if>
						<t:if test="showApproval4">
							<tr>
								<td><t:label for="approvalOp4" /></td>
								<td><t:textfield t:id="approvalOp4" /></td>
							</tr>
						</t:if>
						<t:if test="showApproval5">
							<tr>
								<td><t:label for="approvalOp5" /></td>
								<td><t:textfield t:id="approvalOp5" /></td>
							</tr>
						</t:if>
						<t:if test="showApproval6">
							<tr>
								<td><t:label for="approvalOp6" /></td>
								<td><t:textfield t:id="approvalOp6" /></td>
							</tr>
						</t:if>
						<t:if test="showApproval7">
							<tr>
								<td><t:label for="approvalOp7" /></td>
								<td><t:textfield t:id="approvalOp7" /></td>
							</tr>
						</t:if>
						<t:if test="showApproval8">
							<tr>
