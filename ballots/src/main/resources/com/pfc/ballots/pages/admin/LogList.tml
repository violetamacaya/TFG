<html xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd" xmlns:p="tapestry:parameter"><meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
     <head><title>${message:page-title}</title>
     <link rel="icon" type="image/x-icon" href="${context:favicon2.ico}"/>
	<link rel="icon" href="${context:favicon2.ico}" type="image/x-icon"/>
	</head>
   
	<t:border><div id="userlistdiv">
			
	
			
		<t:zone t:id="logedZone" id="logedZone" t:update="show">
			<h1>${message:users-loged}</h1><hr/>
			<t:grid source="usersLoged" row="userLoged" add="kick" rowsperpage="15" inplace="true">
				<p:kickcell>
					<t:actionlink t:id="kickbut" context="userLoged.idSession" zone="^"><button>${message:kick-but}</button></t:actionlink>
				</p:kickcell>
			</t:grid>
		</t:zone>
		<t:zone t:id="logZone" id="logZone" t:update="show">
			<h1>${message:users-log}</h1><hr/>
			<t:if test="main">
				<t:form>
					<label>${message:show-all}</label><input t:type="checkbox" t:id="showAll" value="showAll" onclick="this.form.submit()"/>
				</t:form>
				<t:grid source="logs" row="log" rowsperpage="15" inplace="true" />
				
				<p:else>
					<t:grid source="logs" row="log" rowsperpage="15" inplace="true" exclude="accessTo" />
				</p:else>
			</t:if>
		</t:zone>
			
			
			
	</div>	
	</t:border>
	
		
			
	


</html>
